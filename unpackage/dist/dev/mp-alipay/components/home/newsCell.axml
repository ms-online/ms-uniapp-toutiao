<view data-event-opts="{{[['tap',[['$emit',['cellTap','$0'],['newsItem.article_url']]]]]}}" class="news-label-cell" onTap="__e"><view class="label-cell-detail"><view class="cell-title">{{''+newsItem.title+''}}</view><block a:if="{{newsItem.image_list&&newsItem.image_list.length>0}}"><view class="list-img"><block a:for="{{newsItem.image_list}}" a:for-item="img" a:for-index="index" a:key="index"><view class="list-img-wrap"><image src="{{img.url}}"></image></view></block></view></block><view class="cell-info"><block a:if="{{newsItem.label}}"><text class="stick-label space">{{newsItem.label}}</text></block><block a:if="{{newsItem.source}}"><text class="src space">{{newsItem.source}}</text></block><block a:if="{{newsItem.comment_count}}"><text class="comment space">{{newsItem.comment_count}}</text></block><block a:if="{{newsItem.datetime}}"><text class="time">{{newsItem.datetime}}</text></block></view></view><block a:if="{{newsItem.image_url}}"><view class="img-holder"><image src="{{newsItem.image_url}}"></image></view></block></view>